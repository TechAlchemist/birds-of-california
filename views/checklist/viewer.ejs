<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include('../partials/cdns.ejs') %>
    <title>Viewer</title>
</head>

<body>
    <h1> Checklist Viewer </h1>
    <% checklists.forEach(list => { %>



        <div class="row">
            <div class="col s12 m6">
              <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                  <span class="card-title"><%=list.listTitle%></span>
                  <p> Created: <%= list.createdAt.toLocaleString()%> </p>
                  <p> Last Updated: <%= list.updatedAt.toLocaleString()%> </p>
                </div>
                <div class="card-action">
                    <a href="/checklist/<%= list._id %>"> Delete List </a>
                    <a href="/checklist/<%= list._id %>"> View Checklist </a>
                </div>
              </div>
            </div>
          </div>
          <% }) %>










</body>

</html>